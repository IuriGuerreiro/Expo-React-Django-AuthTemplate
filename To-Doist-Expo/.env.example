# Backend Configuration
EXPO_PUBLIC_API_BASE_URL=http://192.168.3.2:8000

# Google OAuth Configuration
# Get this from Google Cloud Console - OAuth 2.0 Client IDs
# Should be the same Web Client ID used in your React app
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Optional: Android Client ID (for production APK builds)
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your-android-client-id.apps.googleusercontent.com

# Optional: iOS Client ID (for production iOS builds)  
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your-ios-client-id.apps.googleusercontent.com

# Development Settings
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_DEBUG_MODE=true